{
  "manifestVersion": 1,
  "version": "1.0.22",
  "id": "azure-essence-analytics",
  "name": "Azure Essence Analytics",
  "description": "",
  "publisher": "HITS",
  "demands": [
    "api-version/3.0",
    "contribution/HITS.azure-essence-analytics.essence-hub"
  ],
  "categories": [
    "Azure Boards"
  ],
  "public": false,
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "files": [
    {
      "path": "dist",
      "addressable": true
    }
  ],
  "contributions": [
    {
      "id": "essence-hub",
      "type": "ms.vss-web.hub",
      "targets": [
        "ms.vss-work-web.work-hub-group"
      ],
      "properties": {
        "name": "Essence",
        "uri": "dist/essence-hub/essence-hub.html"
      }
    }
  ],
  "scopes": [
    "vso.work_full",
    "vso.project_write",
    "vso.connected_server",
    "vso.serviceendpoint_manage",
    "vso.project_manage",
    "vso.taskgroups_manage",
    "vso.identity_manage"
  ]
}
